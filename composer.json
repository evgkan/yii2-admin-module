{
    "name": "evgkan/yii2-admin-module",
    "description": "Admin module for Yii2 framework",
    "license": "BSD-3-Clause",
    "type": "yii2-extension",
    "authors": [
        {
            "name": "Pavel Kilin",
            "email": "pavel@asdf404.net"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "rmrevin/yii2-fontawesome": "2.2.1",
        "cakebake/yii2-bootstrap-select": "*"
    },
    "autoload": {
        "psr-4": {
            "asdfstudio\\admin\\": ""
